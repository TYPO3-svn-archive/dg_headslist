<T3DataStructure>
  <sheets>
	<s_general>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.sheet_template</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<template_file>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.template_file_ref</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>tmpl,tmp,html,htm</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</template_file>
				<imageMaxWidth>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.imageWidth</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</imageMaxWidth>
				<imageMaxHeight>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.imageHeight</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</imageMaxHeight>
				<pages>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>3</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</pages>
				<groupMember>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.groupMember</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</groupMember>
				<main_css>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.main_css</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>css</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</main_css>
			</el>
		</ROOT>
	</s_general>
	<s_tooltip>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.sheet_tooltip</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<useToolTips>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.useTips</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</useToolTips>
				<ToolTip_css>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.ToolTip_css</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>css</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</ToolTip_css>
			</el>
		</ROOT>
	</s_tooltip>
</sheets>
</T3DataStructure>