<T3DataStructure>
<meta>
	<langDisable>1</langDisable>
</meta>
  <sheets>
	<s_general>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.sheet_template</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<imageMaxWidth>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.imageWidth</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</imageMaxWidth>
				<imageMaxHeight>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.imageHeight</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</imageMaxHeight>
				<pages>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</pages>
				<defaultCategory>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.defaultCategory</label>
						<config>
					    	<type>select</type>
					    	<itemsProcFunc>tx_dgheadslist_addCategoriesToFlexForm->addCategories</itemsProcFunc>
					    </config>
					</TCEforms>
				</defaultCategory>
				<categoryList>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.categoryList</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</categoryList>
				<inactiveImages>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.inactiveImages</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</inactiveImages>
				<activeLink>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.activeLink</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</activeLink>
				<langOverlay>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.langOverlay</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</langOverlay>
				<template_file>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.template_file_ref</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>tmpl,tmp,html,htm</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</template_file>
			</el>
		</ROOT>
	</s_general>
	<s_tooltip>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.sheet_tooltip</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<useToolTips>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.useTips</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</useToolTips>
				<activeToolTips>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.activeToolTips</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</activeToolTips>
			</el>
		</ROOT>
	</s_tooltip>
</sheets>
</T3DataStructure>