<T3DataStructure>
<meta>
	<langDisable>1</langDisable>
</meta>
  <sheets>
	<sDEF>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.sdef</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<what_to_display>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.what_to_display</label>
						<config>
							<type>select</type>
							<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">NORMAL</numIndex>
								<numIndex index="1">NORMAL</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">GROUPMENU</numIndex>
								<numIndex index="1">GROUPMENU</numIndex>
							</numIndex>
							</items>
							<maxitems>1</maxitems>
							<size>1</size>
						</config>
					</TCEforms>
				</what_to_display>
				<storage_pid>
					<TCEforms>
						<exclude>1</exclude>
						<label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</storage_pid>
				<image_max_width>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.image_max_width</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</image_max_width>
				<image_max_height>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.image_max_height</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</image_max_height>
				<default_group>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.default_group</label>
						<config>
					    	<type>select</type>
					    	<itemsProcFunc>tx_dgheadslist_addGroupsToFlexForm->addGroups</itemsProcFunc>
					    </config>
					</TCEforms>
				</default_group>
				<group_list>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.group_list</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</group_list>
				<inactive_images>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.inactive_images</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</inactive_images>
				<active_link>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.active_link</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</active_link>
				<lang_overlay>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.lang_overlay</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</lang_overlay>
				<normal_pid>
					<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.normal_pid</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
					</config>
					</TCEforms>
				</normal_pid>
				<link_param>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.link_param</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</link_param>
				<template_file>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.template_file_ref</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
							<wizards type="array">
								<link type="array">
									<type>popup</type>
									<title>Link</title>
									<icon>link_popup.gif</icon>
									<script>browse_links.php?mode=wizard&amp;act=file</script>
									<params type="array">
										<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
										<allowedExtensions>tmpl,tmp,html,htm</allowedExtensions>
									</params>
									<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								</link>
							</wizards>
						</config>
					</TCEforms>
				</template_file>
			</el>
		</ROOT>
	</sDEF>
	<sTIP>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.stip</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<use_tooltip>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.use_tooltip</label>
						<config>
							<default>1</default>
							<type>check</type>
						</config>
					</TCEforms>
				</use_tooltip>
				<active_tooltip>
					<TCEforms>
						<label>LLL:EXT:dg_headslist/locallang.xml:pi_flexform.active_tooltip</label>
						<config>
							<default>0</default>
							<type>check</type>
						</config>
					</TCEforms>
				</active_tooltip>
			</el>
		</ROOT>
	</sTIP>
</sheets>
</T3DataStructure>